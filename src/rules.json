[{  
    "id": 1,
    "priority": 1,
    "condition": {  
        "regexFilter": "^https?://.*/.*\\.(mpd|m3u8?|Manifest).*",
        "requireCapturing": true,
        "resourceTypes":[  
           "main_frame"
        ]
    },
    "action": {  
       "type": "redirect",
       "redirect": {
           "extensionPath": "/index.html",
           "regexSubstitution": "index.html#\\0"
       }
    }
}]
